{
    "name": "partybussen/nova-2fa",
    "description": "Laravel Nova 4 Two-factor Authentication Tool.",
    "keywords": [
        "laravel",
        "nova",
	"2fa"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/google2fa-laravel": "^1.0",
        "pragmarx/recovery": "^0.2.1"
    },
    "require-dev": {
        "laravel/nova": "^4.21"
    },
    "autoload": {
        "psr-4": {
            "Partybussen\\Nova2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Partybussen\\Nova2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
